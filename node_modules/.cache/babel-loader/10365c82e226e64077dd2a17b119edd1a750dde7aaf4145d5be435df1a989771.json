{"ast":null,"code":"import \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.map.js\";\nimport axios from \"axios\";\nimport muyalImage from \"../assets/muyal-ilal-logo.png\";\nimport katex from \"katex\";\nimport \"katex/dist/katex.min.css\";\nexport default {\n  data() {\n    return {\n      titulo: \"Space-time proximity indicator to carcinogenic substances (IARC) C50 cancer precursors (SSI)\",\n      muyalImage,\n      Space: \"Space:\",\n      Temporary: \"Temporary\",\n      weights: \"weights associated with risk calculation\",\n      sustancias: [],\n      estados: [],\n      selectedSpace: null,\n      selectedYear: 2013 // Año inicial seleccionado\n    };\n  },\n  methods: {\n    updateRangeValue() {\n      console.log(\"Año seleccionado:\", this.selectedYear);\n    }\n  },\n  mounted() {\n    axios.get(\"http://127.0.0.1:5000/api/sustancias\").then(response => {\n      this.sustancias = response.data.sustancias.map(sustancia => ({\n        ...sustancia,\n        valor: (1 / 17).toFixed(2)\n      }));\n      this.estados = response.data.estados;\n    }).catch(error => {\n      console.error(\"Error fetching data:\", error);\n    });\n    katex.render(\"IPS(S_{iss}) = \\\\sum_{i=1}^{17} \\\\omega_i v_i\", this.$refs.formula, {\n      throwOnError: false\n    });\n    katex.render(\"v_i = f(x_i) = \\\\omega_1 Vol(x_i, t, p) + \\\\omega_2 Diff(x_i,\\\\hat{y_i},t,p) + f(IARC_{X_i}, t) + d(p,E)\", this.$refs.formula2, {\n      throwOnError: false\n    });\n  }\n};","map":{"version":3,"names":["axios","muyalImage","katex","data","titulo","Space","Temporary","weights","sustancias","estados","selectedSpace","selectedYear","methods","updateRangeValue","console","log","mounted","get","then","response","map","sustancia","valor","toFixed","catch","error","render","$refs","formula","throwOnError","formula2"],"sources":["C:\\Users\\Silvestre\\indicador\\src\\components\\HomePage.vue"],"sourcesContent":["<template>\r\n  <header>\r\n    <h1>{{ titulo }}</h1>\r\n  </header>\r\n\r\n  <section class=\"fondo\">\r\n    <div class=\"row-container\">\r\n      <div class=\"title-left\">{{ Space }}</div>\r\n      <div class=\"title-right\">{{ weights }}</div>\r\n    </div>\r\n\r\n    <div>\r\n      <select v-model=\"selectedSpace\" class=\"form-control-left\" id=\"clave\">\r\n        <option v-for=\"(state, index) in estados\" :key=\"index\" :value=\"state\">\r\n          {{ state }}\r\n        </option>\r\n      </select>\r\n    </div>\r\n\r\n    <!-- Control deslizante para \"Temporary\" -->\r\n    <div class=\"col-12 mb-3\">\r\n      <label for=\"temp\" class=\"form-label\">Temporary:</label>\r\n      <input\r\n        type=\"range\"\r\n        class=\"form-control-range\"\r\n        id=\"temp\"\r\n        min=\"2004\"\r\n        max=\"2022\"\r\n        v-model=\"selectedYear\"\r\n        @input=\"updateRangeValue\"\r\n      />\r\n      <div class=\"mt-2\">\r\n        <span>Selected Year: <span id=\"rangeValue\">{{ selectedYear }}</span></span>\r\n      </div>\r\n    </div>\r\n  </section>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\nimport muyalImage from \"../assets/muyal-ilal-logo.png\";\r\nimport katex from \"katex\";\r\nimport \"katex/dist/katex.min.css\";\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      titulo: \"Space-time proximity indicator to carcinogenic substances (IARC) C50 cancer precursors (SSI)\",\r\n      muyalImage,\r\n      Space: \"Space:\",\r\n      Temporary: \"Temporary\",\r\n      weights: \"weights associated with risk calculation\",\r\n      sustancias: [],\r\n      estados: [],\r\n      selectedSpace: null,\r\n      selectedYear: 2013, // Año inicial seleccionado\r\n    };\r\n  },\r\n  methods: {\r\n    updateRangeValue() {\r\n      console.log(\"Año seleccionado:\", this.selectedYear);\r\n    },\r\n  },\r\n  mounted() {\r\n    axios\r\n      .get(\"http://127.0.0.1:5000/api/sustancias\")\r\n      .then((response) => {\r\n        this.sustancias = response.data.sustancias.map((sustancia) => ({\r\n          ...sustancia,\r\n          valor: (1 / 17).toFixed(2),\r\n        }));\r\n        this.estados = response.data.estados;\r\n      })\r\n      .catch((error) => {\r\n        console.error(\"Error fetching data:\", error);\r\n      });\r\n\r\n    katex.render(\r\n      \"IPS(S_{iss}) = \\\\sum_{i=1}^{17} \\\\omega_i v_i\",\r\n      this.$refs.formula,\r\n      { throwOnError: false }\r\n    );\r\n    katex.render(\r\n      \"v_i = f(x_i) = \\\\omega_1 Vol(x_i, t, p) + \\\\omega_2 Diff(x_i,\\\\hat{y_i},t,p) + f(IARC_{X_i}, t) + d(p,E)\",\r\n      this.$refs.formula2,\r\n      { throwOnError: false }\r\n    );\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n/* Personalización del control deslizante */\r\ninput[type=\"range\"] {\r\n  width: 100%;\r\n  accent-color: #007bff;\r\n}\r\n\r\n.col-12 {\r\n  padding: 10px;\r\n}\r\n</style>\r\n"],"mappings":";;AAuCA,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,UAAS,MAAO,+BAA+B;AACtD,OAAOC,KAAI,MAAO,OAAO;AACzB,OAAO,0BAA0B;AAEjC,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,MAAM,EAAE,8FAA8F;MACtGH,UAAU;MACVI,KAAK,EAAE,QAAQ;MACfC,SAAS,EAAE,WAAW;MACtBC,OAAO,EAAE,0CAA0C;MACnDC,UAAU,EAAE,EAAE;MACdC,OAAO,EAAE,EAAE;MACXC,aAAa,EAAE,IAAI;MACnBC,YAAY,EAAE,IAAI,CAAE;IACtB,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,gBAAgBA,CAAA,EAAG;MACjBC,OAAO,CAACC,GAAG,CAAC,mBAAmB,EAAE,IAAI,CAACJ,YAAY,CAAC;IACrD;EACF,CAAC;EACDK,OAAOA,CAAA,EAAG;IACRhB,KAAI,CACDiB,GAAG,CAAC,sCAAsC,EAC1CC,IAAI,CAAEC,QAAQ,IAAK;MAClB,IAAI,CAACX,UAAS,GAAIW,QAAQ,CAAChB,IAAI,CAACK,UAAU,CAACY,GAAG,CAAEC,SAAS,KAAM;QAC7D,GAAGA,SAAS;QACZC,KAAK,EAAE,CAAC,IAAI,EAAE,EAAEC,OAAO,CAAC,CAAC;MAC3B,CAAC,CAAC,CAAC;MACH,IAAI,CAACd,OAAM,GAAIU,QAAQ,CAAChB,IAAI,CAACM,OAAO;IACtC,CAAC,EACAe,KAAK,CAAEC,KAAK,IAAK;MAChBX,OAAO,CAACW,KAAK,CAAC,sBAAsB,EAAEA,KAAK,CAAC;IAC9C,CAAC,CAAC;IAEJvB,KAAK,CAACwB,MAAM,CACV,+CAA+C,EAC/C,IAAI,CAACC,KAAK,CAACC,OAAO,EAClB;MAAEC,YAAY,EAAE;IAAM,CACxB,CAAC;IACD3B,KAAK,CAACwB,MAAM,CACV,0GAA0G,EAC1G,IAAI,CAACC,KAAK,CAACG,QAAQ,EAC